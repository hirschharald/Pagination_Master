const [pager, setPager] = useState({ totalPages: 0 });

  const [items, initialPage, pageSize] = props;

  const setPage = page => {
    if (page < 1 || page > pager.totalPages) {
      return;
    }
  };

  if (items && items.length) {
    setPage(initialPage);
  }
  useEffect(() => {
    setPage(initialPage);
  }, [items, initialPage]);